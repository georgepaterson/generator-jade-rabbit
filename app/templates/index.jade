doctype 5
html.no-js(lang='en')
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		title Jade Rabbit
		link(rel='stylesheet', href='styles/main.css')<% if (modernizr) { %>
		script(src='bower_components/modernizr/modernizr.js')
		<% } %>
	body
		.main.container(role='main')
			.row
				.span12
					p Hello!
			
		script(src='bower_components/jquery/jquery.js')<% if (bootstrap) { %>
		script(src='bower_components/bootstrap/js/bootstrap-affix.js')
		script(src='bower_components/bootstrap/js/bootstrap-alert.js')
		script(src='bower_components/bootstrap/js/bootstrap-dropdown.js')
		script(src='bower_components/bootstrap/js/bootstrap-tooltip.js')
		script(src='bower_components/bootstrap/js/bootstrap-modal.js')
		script(src='bower_components/bootstrap/js/bootstrap-transition.js')
		script(src='bower_components/bootstrap/js/bootstrap-button.js')
		script(src='bower_components/bootstrap/js/bootstrap-popover.js')
		script(src='bower_components/bootstrap/js/bootstrap-carousel.js')
		script(src='bower_components/bootstrap/js/bootstrap-scrollspy.js')
		script(src='bower_components/bootstrap/js/bootstrap-collapse.js')
		script(src='bower_components/bootstrap/js/bootstrap-tab.js')<% } %>
		script(src='scripts/main.js')